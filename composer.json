{
    "name": "bioture/api",
    "description": "Bioture API",
    "type": "project",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Bioture\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bioture\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^4.2",
        "doctrine/doctrine-bundle": "^3.1",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3.5",
        "symfony/console": "8.0.*",
        "symfony/dotenv": "8.0.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "8.0.*",
        "symfony/runtime": "8.0.*",
        "symfony/serializer": "^8.0",
        "symfony/validator": "^8.0",
        "symfony/yaml": "8.0.*"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "platform": {
            "php": "8.5.0"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "deptrac/deptrac": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "friendsofphp/php-cs-fixer": "^3.91",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpunit/phpunit": "^12.5",
        "rector/rector": "^2.2",
        "symfony/maker-bundle": "^1.65",
        "zenstruck/foundry": "^2.8"
    }
}
